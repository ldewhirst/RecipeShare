<div class="container">
  <div class="row row-centered">
    <div class="col-md-6 col-md-offset-4">
      <h1>My Recipes</h1>
    </div>
  </div>
</div>
<hr>
<div class="container">
  <% @user_recipes.each do |recipe| %>
          <div class="box panel-default">
            <div class="thumb">
              <%= image_tag(recipe.image_url(:thumb)) %>
            </div>
            <div class="panel-body text-wrap">
              <%= link_to recipe.title, recipe %>
            </div>
          </div>
        <% end %>
  </div>
</div>
<div class="grid-item--width2">
</div>
<div class="container">
  <div class="row row-centered">
    <div class="col-md-6 col-md-offset-4">
      <h1>Liked Recipes</h1>
    </div>
  </div>
</div>
<hr>
      <div class="container">
          <% @liked_recipes.each do |recipe| %>
                  <div class="box panel-default">
                    <div class="thumb">
                      <%= image_tag(recipe.image_url(:thumb)) %>
                    </div>
                    <div class="panel-body text-wrap">
                      <%= link_to recipe.title, recipe %>
            <%= render partial: 'likes/like', locals: { recipe: recipe } %></br>
        </div>
      </div>
          <% end %>
      </div>
